{
  "name": "MindlyStreamTest",
  "code-version": "1244",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "jetifier",
    "start": "vite --host",
    "ionic:serve": "vite preview",
    "iostart": "CHOKIDAR_USEPOLLING=1 ionic serve",
    "build": "vite build",
    "ionic:build": "tsc && vite build",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)'",
    "test:ts": "tsc --noEmit --skipLibCheck",
    "test:lint": "eslint \"./src/**/*{ts,tsx}\"",
    "coverage": "npm test -- --env=jsdom --coverage --coverageDirectory=jest-coverage",
    "report:combined": "npx nyc report --reporter=lcov --reporter=text --report-dir=coverage --temp-dir=\"./coverage\"\n",
    "codecov": "./node_modules/.bin/codecov --disable=gcov -f ./coverage.json",
    "make-badges": "node lib/index.js -- --functionsLabel='Mis funciones!' --branchesLabel='Branches are troublesome!'",
    "release": "auto shipit",
    "pretty": "prettier --write ./src",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx},!(node_modules/**/*)*.*": "eslint",
    "*.{js,jsx,ts,tsx,md,html,css},!(node_modules/**/*)*.*": "prettier --write"
  },
  "dependencies": {
    "@capacitor/android": "6.1.2",
