    "@ionic-native/qr-scanner": "5.36.0",
    "@ionic/react": "6.7.5",
    "@ionic/react-router": "6.7.5",
    "@mindly/stream-video-call-capacitor": "file:video-call-capacitor-plugin",
    "capacitor-native-settings": "6.0.1",
    "cordova-plugin-androidx": "3.0.0",
    "cordova-plugin-androidx-adapter": "1.1.3",
